{
    "name": "ricardl/levent-bundle",
    "description": "Symfony Bundle for the league/event.",
    "homepage": "https://github.com/ricardl/levent-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "ricardl"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ricardl\\LeventBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ricardl\\LeventBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.2",
        "league/event": "~2.0",
        "symfony/framework-bundle": "4.4.*",
        "symfony/yaml": "4.4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3"
        }
    }
}
